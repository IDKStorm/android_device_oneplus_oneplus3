#[src]                                    [mnt_point]      [type]  [mnt_flags] [fs_mgr_flags]
/dev/block/bootdevice/by-name/boot        /boot            emmc    defaults
/dev/block/bootdevice/by-name/cache       /cache           ext4    defaults
/dev/block/bootdevice/by-name/userdata    /data            ext4    defaults    encryptable=footer
/dev/block/bootdevice/by-name/misc        /misc            emmc    defaults
/dev/block/bootdevice/by-name/persist     /persist         ext4    defaults    display="Persist"
/dev/block/bootdevice/by-name/recovery    /recovery        emmc    defaults    backup=1
/dev/block/bootdevice/by-name/system      /system_root     ext4    defaults    display="System"
/dev/block/bootdevice/by-name/system      /system_image    emmc    defaults
/dev/block/bootdevice/by-name/modemst1    /efs1            emmc    defaults    backup=1,display=EFS
/dev/block/bootdevice/by-name/modemst2    /efs2            emmc    defaults    backup=1,subpartitionof=/efs1
/dev/block/bootdevice/by-name/fsg         /efsg            emmc    defaults    backup=1,subpartitionof=/efs1
/dev/block/bootdevice/by-name/fsc         /efsc            emmc    defaults    backup=1,subpartitionof=/efs1

# USB OTG
/devices/soc/6a00000.ssusb/6a00000.dwc3/xhci-hcd.0.auto/usb*    auto    vfat    defaults    voldmanaged=usbotg:auto,display="USB-OTG",storage,wipeingui,removable
